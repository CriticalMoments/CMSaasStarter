    "@types/html-to-text": "^9.0.4",
    "@types/jsdom": "^21.1.7",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.19.0",
    "autoprefixer": "^10.4.15",
    "daisyui": "^4.7.3",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^9.1.0",
